<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Homepage">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="404dp"
        android:layout_height="86dp"
        android:layout_marginTop="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <Button
        android:id="@+id/btn_checkInOut"
        android:layout_width="165dp"
        android:layout_height="50dp"
        android:layout_marginTop="45dp"
        android:text="Check In/ Out"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <Button
        android:id="@+id/btn_reservation"
        android:layout_width="165dp"
        android:layout_height="50dp"
        android:layout_marginTop="45dp"
        android:text="Reservation"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_checkInOut" />

    <Button
        android:id="@+id/btn_uProfile"
        android:layout_width="165dp"
        android:layout_height="50dp"
        android:layout_marginTop="45dp"
        android:text="User Profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_dining" />

    <Button
        android:id="@+id/btn_dining"
        android:layout_width="165dp"
        android:layout_height="50dp"
        android:layout_marginTop="45dp"
        android:text="Dining Service"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_reservation" />

    <Button
        android:id="@+id/btn_logout"
        android:layout_width="165dp"
        android:layout_height="50dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="44dp"
        android:text="Logout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_uProfile" />
</androidx.constraintlayout.widget.ConstraintLayout>